<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav_component"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.androidnavigationcomponentapp.fragment.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/feedFragment"
        android:name="com.example.androidnavigationcomponentapp.fragment.FeedFragment"
        android:label="Feed"
        tools:layout="@layout/fragment_feed" />
    <fragment
        android:id="@+id/officialStoreFragment"
        android:name="com.example.androidnavigationcomponentapp.fragment.OfficialStoreFragment"
        android:label="Official Store"
        tools:layout="@layout/fragment_official_store" />
    <fragment
        android:id="@+id/wishListFragment"
        android:name="com.example.androidnavigationcomponentapp.fragment.WishListFragment"
        android:label="Wishlist"
